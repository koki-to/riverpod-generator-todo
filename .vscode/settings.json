{
    "dart.flutterSdkPath": ".fvm/flutter_sdk",
    "search.exclude": {
        "**/.fvm": true
    },
    "files.watcherExclude": {
        "**/.fvm": true
    },
    "files.exclude": {
        "**/*.freezed.dart": true,
        "**/*.g.dart": true
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
}